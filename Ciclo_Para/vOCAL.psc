Proceso vOCAL
	Definir x Como Entero;
	
	Definir c Como Caracter;
	
	x <- 1;
	
	Mientras x <> 0 Hacer
		
		Escribir "Ingresa una letra";
		
		leer c;
		
		si c="a" o c= "e" o c=  "i" o c=  "o" o c = "u" Entonces;
			
			x<-  0;
			
		SiNo
			
			si c="A" o c= "E" o c=  "I" o c=  "O" o c = "U" Entonces
				
			FinSi
			
		FinSi

          FinMientras
FinProceso
